<h2><%= @PAGE_TITLE = "New Output" %></h2>

<%= simple_form_for [@output], html: { class: "form-horizontal" } do |f| %>
    <%= f.error_notification %>

    <fieldset>
        <%= render 'form_fields', f: f %>

        <div class="form-actions">
            <%= f.submit "Save", class: "btn" %>
        </div>
    </fieldset>
<% end %>
