<% @PAGE_TITLE = "New Output" %>

<h1>New Output</h1>

<%= simple_form_for @output, :url => a_outputs_path, :html => {:class => "form-horizontal"} do |f| %>
    <fieldset>
        <legend>Output</legend>
        <%= f.input :code %>
        <%= f.input :size %>
        <%= f.input :extension %>
        <%= f.input :prerender %>
        <%= f.input :is_rich %>
        <div class="form-actions">
            <%= f.submit %>
        </div>
    </fieldset>
<% end %>
